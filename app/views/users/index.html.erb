<div class="main">
	<div class="container">
		<div class="row">
			<% if @users.any? %>
				<% @users.each do |user| %>
				<li><%= user.name %></li>
				<p>current user:<%= current_user.name unless current_user.nil? %></p>
				<% end %>
			<% else %>
				<p>no user yet</p>
			<% end %>
		</div>
	</div>
</div>